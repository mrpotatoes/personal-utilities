<%= lib.package.name %>

{
  "name": "<%= lib.package.namespace %>/<%= lib.package.name %>",
  "version": "1.0.0",
  "description": "<%= lib.package.description %>",
  "author": "<%= lib.package.author %>",
  "homepage": "<%= lib.package.homepage %>",
  "license": "MIT",
  "main": "<%= lib.package.mainscript %>",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "<%= lib.package.giturl %>"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/bin.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn src/bin.ts",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "<%= lib.package.gitissues %>"
  },
  "dependencies": {
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@types/jest": "~26.0.4",
    "jest": "~26.1.0",
    "ts-jest": "~26.1.2"
  }
}